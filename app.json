{
  "expo": {
    "name": "foodie",
    "slug": "foodie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saketh264.foodie"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.saketh264.foodie",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAUeeP2v4Esz1VPXLvPhJXryjHqPkHhWG8"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/Poppins.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8ef3f2ac-8acb-4ce6-82f0-c9e40cd4490f"
      }
    },
    "owner": "saketh264"
  }
}
